@model Developer
@{
    ViewData["Title"] = "Index";
}


<div class="col-4 offset-2">
    <h2>Developer Registration</h2>
    <form asp-action="Create" asp-controller="Developer" method="post">
        <div class="form-group">
            <label asp-for="Name">Name</label>
            <input type="text" class="form-control" asp-for="Name">

        </div>

        <div class="form-group">
            <label asp-for="Age">Age</label>
            <input type="text" class="form-control" asp-for="Age">

        </div>
        <div class="form-group">
            <label asp-for="Gender">Gender</label>
            <input type="text" class="form-control" asp-for="Gender">

        </div>
        <div class="form-group">
            <label asp-for="Type">Type</label>
            <input type="text" class="form-control" asp-for="Type">

        </div>

        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="" id="defaultCheck1">
            <label class="form-check-label" for="defaultCheck1">
                Default checkbox
            </label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="" id="defaultCheck2" >
            <label class="form-check-label" for="defaultCheck2">
                Disabled checkbox
            </label>
        </div>

        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>